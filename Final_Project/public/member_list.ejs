                    <div class="contact-form-area">
                        <form action="/member_list_search" method="get">
                            <div class="row">
                                <div class="search-name">
                                    <input type="text" class="form-control-search" name="name" placeholder="Name">
                                    <button class="btn delicious-btn mt-0" type="submit">Search</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
      <div class="container-table100">
          
         <div class="wrap-table100">
             
           <div class="table100 ver2 m-b-0">
               
               <div class="table100-head">
                  <table>
                     <thead>
                        <tr class="row100 head">
                           <th class="cell100 column1s">이름</th>
                           <th class="cell100 column2s">성별</th>
                           <th class="cell100 column3s">생년월일</th>
                           <th class="cell100 column4s">종류</th>
                           <th class="cell100 column5s">센터명</th>
                           <th class="cell100 column6s">알람</th>
                        </tr>
                     </thead>
                  </table>
               </div>

               <div class="table100-body js-pscroll">
                  <table>
                     <tbody>
                          <%for(var i=0;i<results.length;i++){ %>
								<tr class="row100 body">   
									<td class="cell100 column1"onClick = "location.href='/member_change?name=<%= results[i].u_name %>'" ><%= results[i].u_name %></td>
                                    <td class="cell100 column2"onClick = "location.href='/member_change?name=<%= results[i].u_name %>'" ><%= results[i].u_sex %></td>
                                    <td class="cell100 column3"onClick = "location.href='/member_change?name=<%= results[i].u_name %>'" ><%= results[i].u_birth %></td>
                                    <td class="cell100 column4"onClick = "location.href='/member_change?name=<%= results[i].u_name %>'" ><%= results[i].ui_kind %></td>
                                    <td class="cell100 column4"onClick = "location.href='/member_change?name=<%= results[i].u_name %>'" ><%= results[i].ui_gym %></td>
                                    <td><a href="/alarm_class"><img class="alarm" src="img/bg-img/alarm.png" alt=""></a></td>
								</tr>
                                <%}%>
                     </tbody>
                  </table>
               </div>
               
            </div>
               
              </div>
         </div>
    </div>
